{
  "name": "ionic-supabase-server",
  "version": "1.0.0",
  "description": "Servidor Node.js para Ionic Angular App con Supabase y MCP",
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npx ionic build --prod",
    "serve": "npm run build && npm start",
    "deploy": "npm run build && pm2 start server.js --name ionic-supabase-app",
    "prebuild": "npm install -g @ionic/cli@latest",
    "vercel-build": "npm run prebuild && npm run build"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@ionic/angular": "^7.0.0",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "helmet": "^7.0.0",
    "path": "^0.12.7",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular/cli": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.0",
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0",
    "@ionic/cli": "^7.0.0",
    "@analogjs/vite-plugin-angular": "^0.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}